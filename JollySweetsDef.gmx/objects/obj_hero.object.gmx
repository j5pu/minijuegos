        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Colisión con plataformas móviles

//Plataformas con movimiento vertical

if (collision_point(x+20,y+64,updown,false,true)&gt;0)
    y+=instance_nearest(x+20,y+64,updown).vspeed;
else if (collision_point(x-20,y+64,updown,false,true)&gt;0)
    y+=instance_nearest(x-20,y+64,updown).vspeed;
    
    
if (collision_point(x+20,y+64,updown2_up,false,true)&gt;0)
    y+=instance_nearest(x+20,y+64,updown2_up).vspeed;
else if (collision_point(x-20,y+64,updown2_up,false,true)&gt;0)
    y+=instance_nearest(x-20,y+64,updown2_up).vspeed;
    

//Plataformas con movimiento horizontal

if (collision_point(x,y+64,object44,false,true)&gt;0) 
    x+=instance_nearest(x,y+64,object44).hspeed;
    
    
if (collision_point(x,y+64,horizontal_left,false,true)&gt;0) 
    x+=instance_nearest(x,y+64,horizontal_left).hspeed;
</string>
